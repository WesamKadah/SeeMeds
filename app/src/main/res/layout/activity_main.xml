    <LinearLayout
        android:id="@+id/frag_container_layout"
        android:layout_width="match_parent"
        android:layout_height="667dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="-59dp"
        tools:layout_editor_absoluteY="-1dp">

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/fragmentContainerView2"
            android:name="com.example.seemeds.CameraFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_bar_layout"
        android:layout_width="match_parent"
        android:layout_height="63dp"
        android:gravity="bottom"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="-16dp">


        <ImageButton
            android:id="@+id/button_camera"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:src="@drawable/round_camera_alt_24"
            android:text="Camera" />

        <ImageButton
            android:id="@+id/button_schedule"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:src="@drawable/round_calendar_month_24"
            android:text="Button" />

        <ImageButton
            android:id="@+id/button_preferences"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:src="@drawable/round_build_24"
            android:text="Button" />

